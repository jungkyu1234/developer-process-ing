//자바스크립트 홈페이지 몇 초 이후 이동 구현 소스코드

<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
<script>
function kaja() { /* void, static x */
/* 	window.location="http://www.naver.com" */ //객체.속성=값
	location.href="http://www.naver.com"
	/* 객체.속성=값 */
}
</script>
</head>
<body onLoad="setTimeout('kaja()',3000)"> <!-- kaja()는 함수 호출문 -->
<!-- 3000ms 1초 = 1000ms -->
<!-- on이 붙으면 이벤트 -->
<!-- setTimeout() : 한번만, setInterval() : 계속, 시계-->
<h1 align="center">http://www.naver.com으로 이사하였습니다.</h1>
<p><h2> 잠시후 http://www.naver.com으로 이동합니다.</h2></p>
</body>
</html>


==========================================================================================

//자바스크립트 alert 띄우기 소스코드 구현 구문

<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>
<script>
window.alert('안녕 자바스크립트')
/* window는 상태바 모든 부분 */
alert("윈도우생략했네")

document.write("글씨쓰는부분")
/* 하얀색 부분만 document */
alert("윈도우생략했네"); alert("윈도우생략했네") /* 두 문장을 한번에.. ; 사용 */

alert("안녕 자바스크립트"+" 반갑네");

alert("윈도우생략했네",+" 반갑네");

</script>
</body>
</html>
