====스프링 사칙연산 구현하기

package db;

public class Calculation {
	
	private int firstNum;
	private int secondNum;
	
	public final int getFirstNum() {
		return firstNum;
	}
	public final void setFirstNum(int firstNum) {
		this.firstNum = firstNum;
	}
	public final int getSecondNum() {
		return secondNum;
	}
	public final void setSecondNum(int secondNum) {
		this.secondNum = secondNum;
	}
	
	public void add() {
		System.out.println("더하기 양식");
		System.out.println(firstNum + "+" + secondNum + "=" + (firstNum + secondNum));
	}
	public void sub() {
		System.out.println("빼기 양식");
		System.out.println(firstNum + "-" + secondNum + "=" + (firstNum - secondNum));
	}
	public void mult() {
		System.out.println("곱하기 양식");
		System.out.println(firstNum + "*" + secondNum + "=" + (firstNum * secondNum));
	}
	public void div() {
		System.out.println("나누기 양식");
		System.out.println(firstNum + "/" + secondNum + "=" + (firstNum / secondNum));
	}
}



