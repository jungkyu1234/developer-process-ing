====스프링 사칙연산 구현2

package db;

public class MainClass {

	public static void main(String[] args) {
		
		Calculation calculation = new Calculation();
		
		calculation.setFirstNum(2);
		calculation.setSecondNum(10);
		
		calculation.add();
		calculation.sub();
		calculation.mult();
		calculation.div();

	}
}
