$(function() {
$("img").css("width", 100);
$("img").css("height", 100);

setInterval(function () {
	$("img").first().appendTo("body"); //처음 이미지로 1번, 2번, 3번이 왔다갔다 함.
}, 1000); //3초 간격
});
</script>
